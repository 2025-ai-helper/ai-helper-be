{
  "name": "ai-helper-be",
  "version": "0.0.1",
  "description": "AI Helper BE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-helper/ai-helper-be.git"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "start:dev": "nodemon -w ./ -w ../lib --ext ts,js,tsx --exec 'STAGE=local TZ=UTC MODE=instance npm run ts-node src/main.ts'",
    "ts-node": "ts-node -r tsconfig-paths/register --files",
    "--------": "echo 1",
    "lint": "maven-linter lint",
    "lint:fix": "maven-linter lint:fix",
    "---------": "echo 1",
    "migrate:db:local": "dotenv -e .env.local -- knex-automigrate migrate:auto --knexfile ./migration/knexfile.js --cwd ./migration",
    "migrate:db:develop": "dotenv -e .env.develop -- knex-automigrate migrate:auto --knexfile ./migration/knexfile.js --cwd ./migration"
  },
  "dependencies": {
    "@actions/github": "^6.0.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "mysql2": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
